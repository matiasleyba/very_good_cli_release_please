{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "changelog-types": [
    { "type": "feat", "section": "Features" },
    { "type": "fix", "section": "Fixes" },
    { "type": "refactor", "section": "Refactors" },
    { "type": "chore", "section": "Miscellaneous" },
    { "type": "docs", "section": "Documentation" }
  ],
  "pull-request-header": ":rotating_light: There are changes ready for release :rocket:\n\nMerge this PR once the team confirms the release is ready.",
  "pull-request-title-pattern": "chore: ${version}",
  "include-component-in-tag": false,
  "packages": {
    ".": {
      "release-type": "dart",
      "changelog-path": "CHANGELOG.md",
      "version-file": "pubspec.yaml"
    }
  }
}